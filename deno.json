{
  "tasks": {
    "dev": "deno run --watch --allow-read --allow-env main.ts",
    "format": "deno fmt"
  },
  "compilerOptions": {
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "imports": {
    "@logtape/logtape": "jsr:@logtape/logtape@^0.9.0",
    "@std/assert": "jsr:@std/assert@1"
  }
}
